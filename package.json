{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "build": "vite build",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progresss --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
